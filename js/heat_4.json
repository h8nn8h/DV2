        "color": {"field": "sst", "type": "quantitative", "scale": {"scheme": "viridis"}},
        "tooltip": [
          {"field": "Latitude", "type": "quantitative"},
          {"field": "Longitude", "type": "quantitative"},
          {"field": "Season", "type": "nominal"}
        ]
      }
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/h8nn8h/Week_9_Homework/refs/heads/main/js/Aus_Map.json",
        "format": {"type": "topojson", "feature": "AUS_2021_AUST_GDA2020"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    }
  ]
}