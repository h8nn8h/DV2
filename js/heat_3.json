{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 500,
  "title": "Check SST Rectangles",
  "projection": {
    "type": "equirectangular",
    "center": [133, -28],
    "scale": 567
  },
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/h8nn8h/DV2/main/js/Aus_Map.json",
        "format": {"type": "topojson", "feature": "AUS_2021_AUST_GDA2020"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/h8nn8h/DV2/refs/heads/main/js/sst_rectangles.json",
        "format": {"type": "topojson", "feature": "sst_rectangles"}
      },
      "mark": {"type": "geoshape", "stroke": "red"},
      "encoding": {
        "color": {
          "field": "sst",
          "type": "quantitative",
          "scale": {"scheme": "viridis"}
        },
        "tooltip": [
          {"field": "sst", "type": "quantitative"},
          {"field": "season", "type": "nominal"},
          {"field": "lat", "type": "quantitative"},
          {"field": "lon", "type": "quantitative"}
        ]
      }
    }
  ]
}
